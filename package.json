{
  "name": "gatsby-starter-mate",
  "version": "0.0.0",
  "description": "An accessible and fast portfolio starter for Gatsby integrated with Contentful CMS",
  "license": "MIT",
  "scripts": {
    "start": "env-cmd --silent gatsby develop",
    "serve": "env-cmd --silent gatsby serve",
    "build": "env-cmd --silent gatsby build",
    "deploy": "gh-pages -d public -b deploy",
    "clean": "gatsby clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://https://github.com/blakeaber/blakeaber.github.io.git"
  },
  "keywords": [
    "gatsby",
    "contentful",
    "resume",
    "portfolio"
  ],
  "author": "Blake Aber",
  "homepage": "https://https://github.com/blakeaber/blakeaber.github.io",
  "dependencies": {
    "gatsby": "^4.24.8",
    "gatsby-plugin-google-analytics": "^4.24.0",
    "gatsby-theme-mate": "^2.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/inquirer": "^9.0.3",
    "chalk": "^4.1.2",
    "contentful-import": "^8.5.13",
    "env-cmd": "^10.1.0",
    "gh-pages": "^4.0.0",
    "inquirer": "^9.1.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
